    }
  }
  export default new Method();
</script>

<script cml-type="wx">
  class Method implements UtilsInterface {
    navigateBack(backPageNum) {
      wx.navigateBack({
        delta: Math.abs(backPageNum)
      });
    }
  }
  export default new Method();
</script>

