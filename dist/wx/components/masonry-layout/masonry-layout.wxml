<view class="masonry-box  cml-base cml-view" style="width:{{masonryBoxWidth}}rpx;right:{{rightDistance}}px">
  <view class="masonry-item  cml-base cml-view  _cml_ref_lmc_" wx:for="{{masonryList}}" wx:key="{{item.uuid}}" style="height:{{item.h}}rpx;width:{{item.w}}rpx;top: {{item.top}}rpx;left: {{item.left}}rpx" id="masonry-item-{{index}}">
    <slot name="masonry-item-{{index}}" class=" cml-base cml-slot"></slot>
  </view>
  <view class="loading-box  cml-base cml-view" style="top:{{loadingPos}}rpx" wx:if="{{loading}}"><slot name="loading" class=" cml-base cml-slot"></slot></view>
  <view class="white-space  cml-base cml-view" style="top:{{whiteSpace}}rpx" wx:if="{{loading}}"></view>
</view>