<view class="masonry-box  cml-base cml-view cml-13ac27bb" style="width:{{masonryBoxWidth}}rpx;right:{{rightDistance}}px">
  <view class="masonry-item  cml-base cml-view cml-13ac27bb  _cml_ref_lmc_" a:for="{{masonryList}}" a:key="{{item.uuid}}" style="height:{{item.h}}rpx;width:{{item.w}}rpx;top: {{item.top}}rpx;left: {{item.left}}rpx" id="masonry-item-{{index}}">
    <slot name="masonry-item-{{index}}" class=" cml-base cml-slot cml-13ac27bb"></slot>
  </view>
  <view class="loading-box  cml-base cml-view cml-13ac27bb" style="top:{{loadingPos}}rpx" a:if="{{loading}}"><slot name="loading" class=" cml-base cml-slot cml-13ac27bb"></slot></view>
  <view class="white-space  cml-base cml-view cml-13ac27bb" style="top:{{whiteSpace}}rpx" a:if="{{loading}}"></view>
</view>